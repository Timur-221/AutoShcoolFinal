<Page x:Class="AutoShcool.Pages.Ter2Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:ma="http://metro.mahapps.com/winfx/xaml/iconpacks"
      xmlns:local="clr-namespace:AutoShcool.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Ter2Page">

    <Grid Background="Wheat">
        <StackPanel>
            <ma:PackIconMaterial Kind="ArrowCollapseLeft" Width="50" Height="50" MouseDown="PDDNavBack1"/>
        </StackPanel>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="Нет доступных зачётов или экзаменов, обратитесь в автошколу." FontSize="28" TextWrapping="Wrap"></TextBlock>
    </Grid>
</Page>
